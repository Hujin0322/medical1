  <script async="false" id="id_cp_i18n_replace_locale">
   function __cp_i18n_replace_locale__(css) { 	    var r = css, locale = ''; 	    var groups = /x-coupang-accept-language=([w-]+)/.exec(document.cookie); 	    if (groups && groups[1]) { 	      locale = groups[1]; 	    } else { 	      locale = 	        window['x-coupang-accept-language'] || 	        document.documentElement.getAttribute('lang') || 	        'ko_KR'; 		} 				locale = locale.replace(/(.+)-(.+)/g, '$1_$2'); 	  	    if (r) { 	      if (/^ko/.test(locale)) { 	        r = r.replace(/<!--LOCALE-->\//gm, ''); 	      } else { 	        r = r.replace(/<!--LOCALE-->/gm, locale); 	      } 	    } 	  	    return r; 	}
  </script>
